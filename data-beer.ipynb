    "import pandas as pd\n",
    "\n",
    "df = pd.read_csv('merged_data.csv')\n",
    "\n",
    "#delete nan values\n",
    "df = df.dropna()\n",
    "\n",
    "#Create a new column with the money spent per capita\n",
    "df['money_spent_per_capita'] = df['Beer Consumption'] * df['Beer Price'] * 2\n",
    "\n",
    "df"
   ]
